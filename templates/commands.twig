{% extends "layout.twig" %}

{% block content %}
<input type="text" class="typeahead">

<script type="text/javascript">
$(function() {
    localStorage.clear();

    $('.typeahead').typeahead({                                
        name: 'commands',
        prefetch: '/data/commands.json',
        limit: 20
    });

    $('.typeahead').on('typeahead:selected', function (event, datum) {
        window.location.href = datum.path;
    });
});
</script>
{% endblock %}
